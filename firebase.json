{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
		"rewrites": [{
			"source": "**/",
			"destination": "index.html"
		},
		{
			"source": "**/admin",
			"destination": "admin.html"
		}]
  }
}
